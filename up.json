{
  "name": "gql-api",
  "profile": "default",
  "hooks": {
    "build": "yarn run build",
    "clean": "yarn run restore"
  },
  "proxy": {
    "command": "PORT=$PORT NODE_ENV=$UP_STAGE node dist/main"
  },
  "lambda": {
    "memory": 512
  },
  "stages": {
    "production": {
      "domain": "api.wehzhen.com"
    },
    "staging": {
      "domain": "qa.api.wehzhen.com"
    },
    "development": {
      "proxy": {
        "command": "PORT=$PORT NODE_ENV=$UP_STAGE node dist/main"
      }
    }
  }
}
