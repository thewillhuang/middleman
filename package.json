{
  "name": "mass-consensus",
  "version": "1.0.0",
  "main": "dist/main.js",
  "repository": "git@github.com:thewillhuang/mass-consensus.git",
  "author": "William Huang <will.h86@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "postgraphile": "^4.0.0-beta.9"
  },
  "scripts": {
    "start": "yarn clean && webpack --progress --mode=development --watch & yarn dev",
    "dev": "NODE_ENV=development DEBUG='*:sql' nodemon -e js,jsx,sql,json dist/main.js",
    "backup": "mv ./node_modules/ ./node_modules_bak/ && mv ./.yarnclean_bak ./.yarnclean",
    "restore": "rm -fr node_modules && mv ./node_modules_bak/ ./node_modules/ &&  mv ./.yarnclean ./.yarnclean_bak",
    "build": "yarn clean && webpack --mode=production && yarn run backup && yarn --ignore-engines --prod",
    "clean": "rm -fr dist",
    "makeCache": "node dist/makeCache",
    "stage": "up staging",
    "prod": "up production",
    "open": "up url -o",
    "test": "jest --verbose",
    "logs": "up logs -f",
    "db:create": "createdb middleman",
    "db:drop": "dropdb middleman",
    "db:up": "db-migrate up",
    "db:up:test": "db-migrate up -e test",
    "db:r": "yarn db:down && yarn db:drop && yarn db:create && yarn db:up",
    "db:down": "db-migrate down"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-transform-async-to-bluebird": "^1.1.1",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "jest-raw-loader": "^1.0.1",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1",
    "ramda": "^0.25.0",
    "raw-loader": "^0.5.1",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "uuid": "^3.2.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.6"
  },
  "resolutions": {
    "graphql": "^0.13.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.graphql$": "jest-raw-loader",
      "\\.md$": "jest-raw-loader",
      "\\.pem": "jest-raw-loader",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  }
}
